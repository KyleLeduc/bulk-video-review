{
  "name": "bulk-video-review",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dep:src": "depcruise src --include-only \"^src\" --output-type dot | dot -T svg > ./dep-graphs/src-graph.svg",
    "dep:components": "depcruise src --include-only \"^src\" --focus components --output-type dot | dot -T svg > ./dep-graphs/components-graph.svg",
    "dep:stores": "depcruise src --include-only \"^src\" --focus stores --output-type dot | dot -T svg > ./dep-graphs/stores-graph.svg",
    "dep:videosStore": "depcruise src --include-only \"^src\" --focus videosStore --output-type dot | dot -T svg > ./dep-graphs/videosStore-graph.svg",
    "dep-graph": "run-p dep:*",
    "prepare": "husky"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.0",
    "pinia": "^2.0.26",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^11.2.0",
    "dependency-cruiser": "^16.3.1",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^9.0.11",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.77.4",
    "start-server-and-test": "^1.14.0",
    "typescript": "^5.4.5",
    "vite": "^3.2.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.0.9"
  }
}

